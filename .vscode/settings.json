{
    "yaml.schemas": {
        ".vscode/github-workflow-custom.json": [
            ".github/workflows/*.yml"
        ]
    },
    "yaml.customTags": [
        "!reference sequence"
    ],
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "yaml.format.enable": true,
    "yaml.validate": true,
    "yaml.hover": true,
    "yaml.completion": true,
    "yaml.schemaStore.enable": false,
    "github-actions.workflows.pinned-refresh.workflows": [
        ".github/workflows/taskmaster-enhanced.yml",
        ".github/workflows/auto-form-proxy.yml"
    ],
    "github-actions.workflows.customPatterns": {
        "env": ".vscode/github-workflow-env-vars.json"
    },
    "github-actions.workflows.icons.custom": true
}